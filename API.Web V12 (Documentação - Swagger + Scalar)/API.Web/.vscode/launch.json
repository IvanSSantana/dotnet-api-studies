{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "API.Web",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/bin/Debug/net10.0/API.dll",
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "console": "integratedTerminal",
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },

      "logging": {
        "moduleLoad": false,
        "moduleUnload": false,
        "threadStart": false,
        "threadExit": false
      }
    }
  ]
}


